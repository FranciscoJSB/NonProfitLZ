{
    "handler": "Microsoft.Compute.MultiVm",
    "version": "1.0",
    "parameters": {
        "enterpriseScaleCompanyPrefix": {
            "type": "string",
            "metadata": {
                "description": "Provide a prefix (max 10 characters, unique at tenant-scope) for the Management Group hierarchy and other resources created as part of Enterprise-scale.",
                "displayName": "Company Prefix"
            }
        },
        "selectedSubscription": {
            "type": "Microsoft.Common.Subscription",
            "metadata": {
                "description": "Select the subscription to be used dynamically.",
                "displayName": "Select Subscription"
            }
        }
    },
    "outputs": {
        "enterpriseScaleCompanyPrefix": {
            "type": "string",
            "value": "[parameters('enterpriseScaleCompanyPrefix')]"
        },
        "selectedSubscription": {
            "type": "string",
            "value": "[parameters('selectedSubscription')]"
        }
    }
}
